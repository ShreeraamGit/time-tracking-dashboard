{
  "name": "time-tracking-dashboard",
  "version": "1.0.0",
  "description": "This is a solution to the [Time tracking dashboard challenge on Frontend Mentor](https://www.frontendmentor.io/challenges/time-tracking-dashboard-UIQ7167Jw). Frontend Mentor challenges help you improve your coding skills by building realistic projects.",
  "main": "index.js",
  "scripts": {
    "tailwind:build:dev": "npx tailwindcss -i tailwindcss.css -o ./src/styles.css --watch",
    "server": "servor src index.html 8080 --browse --reload",
    "dev": "run-p \"server\" \"tailwind:build:dev\"",
    "tailwind:build:prod": "postcss tailwindcss.css -o ./src/styles.css",
    "prod": "npm run tailwind:build:prod"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ShreeraamGit/time-tracking-dashboard.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/ShreeraamGit/time-tracking-dashboard/issues"
  },
  "homepage": "https://github.com/ShreeraamGit/time-tracking-dashboard#readme",
  "devDependencies": {
    "autoprefixer": "^10.4.1",
    "cssnano": "^5.0.14",
    "npm-run-all": "^4.1.5",
    "postcss-cli": "^9.1.0",
    "servor": "^4.0.2",
    "tailwindcss": "^3.0.11"
  }
}
